#include<iostream>
#include<math.h>
using namespace std;

int factorial(int t){
    int facto=1;
    for(int b=1;b<=t;b++){
        facto=b*facto;
    }
    return facto;
}

int main(){

    int a = 0;
    long double b = 0;
    long double c = 0;

    for(int i = 0; i < 8; i++){
        c = pow(-5,a)/factorial(a);
        b+=c;
        cout<<"    "<<(b*-1)<<endl;
        a++;
    }
    return 0;
}
